<div class="page-header">
  <h1>Projects</h1>

  <ul class="actions">
    <li><%= link_to "New Project", new_project_path, class: 'new' %>
   </li>
  </ul>
</div>

<div id="notice"><%= notice %></div>

<% @projects.each do |project| %>
  <h2><%= link_to project.name, project %> </h2>
  <p><%= project.description %></p>
  <p><em><%= link_to "Update Project", edit_project_path(project) %></em></p>
<% end %> 
